{
  "name": "jupyterlab-collab-mcp",
  "version": "0.8.0",
  "description": "MCP server for real-time JupyterLab notebook collaboration",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianhi/jupyterlab-collab-mcp.git"
  },
  "keywords": [
    "mcp",
    "mcp-server",
    "jupyterlab",
    "jupyter",
    "notebook",
    "ai",
    "real-time",
    "collaboration"
  ],
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "jupyter-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "src",
    "python/src/variable_inspector/inspector.py"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "npx tsx src/test.ts",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:preview": "cd docs && npm run preview"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "ws": "^8.0.0",
    "y-websocket": "^2.0.0",
    "yjs": "^13.6.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/ws": "^8.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "tsx": "^4.0.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
